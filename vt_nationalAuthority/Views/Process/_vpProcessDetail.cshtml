@model DataAccessLayer.Models.processModel
@{
    var screen = "";
    string[] path = HttpContext.Current.Request.Url.AbsolutePath.Split('/');
    screen = path[2];
}
<div class="mt-3 dr-rtl grid_box_shadow mx-3 mb-3 ">
    <div class="row text-right">
        @if (Model.sNameOwnerPermision != "-1")
        {
            @* مكتب *@
            <div class="col-lg-6 col-md-12 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">مكتب : <span class="color_main103"> @Model.sOfficeIDName</span></span>
            </div>
            @* منطقة *@
            <div class="col-lg-6 col-md-12 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">منطقة : <span class="color_main103">@Model.sAreaIDName</span></span>
            </div>
            @* رقم العملية *@
            <div class="col-lg-5 col-md-6 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">رقم العملية :</span>
                <span class="d-inline-flex">  <span class="color_main103 form-control" style="width: fit-content!important">----- </span><span class="color_main103 form-control mr-1" style="width: fit-content!important">----</span></span>
                @*<span class="font-weight-bold">(مبندة)</span>*@
            </div>
        }

        @* اسم العملية *@
        <div class="col-lg-12 col-md-12 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">اسم العملية : <span class="color_main103">@Model.sProcessName</span> </span>
        </div>
        @* نوع العملية *@
        <div class="col-12 my-1">
            <span class="color_main102 font-weight-bold">نوع العملية : <span class="color_main103">@Model.sProcessTypeName</span></span>
        </div>
        @* المقاول الرئيسى *@
        <div class="col-lg-7 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">المقاول الرئيسى : <span class="color_main103">@Model.sContractorName</span></span>
        </div>
        @* الرقم التأمينى *@
        <div class="col-lg-5 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">الرقم التأمينى : <span class="color_main103">@Model.sContractorNum</span></span>
        </div>
        <div class="col-lg-6 col-md-12 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">الكيان القانونى للمقاول الرئيسي : <span class="color_main103">@Model.sContractorLegalEntityName</span></span>
        </div>
        @* تليفون المقاول الرئيسي *@
        <div class="col-lg-7 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">تليفون المقاول الرئيسي: <span class="color_main103">@(String.IsNullOrEmpty(Model.sContractorPhone) ? "------------" : Model.sContractorPhone)</span></span>
        </div>
        @*  رقم فاكس المقاول الرئيسي *@
        <div class="col-lg-5 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">رقم فاكس المقاول الرئيسي : <span class="color_main103">@(String.IsNullOrEmpty(Model.sContractorFax) ? "------------" : Model.sContractorFax)</span></span>
        </div>
        @* ايميل المقاول الرئيسي *@
        <div class="col-lg-7 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">ايميل المقاول الرئيسي: <span class="color_main103">@(String.IsNullOrEmpty(Model.sContractorEmail) ? "------------" : Model.sContractorEmail)</span></span>
        </div>
        @* جهة الإسناد *@
        <div class="col-lg-7 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">جهة الإسناد : <span class="color_main103">@Model.sReferanceSideName</span></span>
        </div>
        @* الرقم التأمينى *@
        <div class="col-lg-5 col-md-6 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">الرقم التأمينى : <span class="color_main103">@Model.sReferanceSideNum</span></span>
        </div>
        <div class="col-lg-6 col-md-12 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">الكيان القانونى لجهة الإسناد : <span class="color_main103">@Model.sReferanceSideLegalEntityName</span></span>
        </div>
        @*نوع المستند*@@*تاريخ نوع المستند*@
        <div class="row col-12">
            @*نوع المستند*@
            <div class="col-lg-7 col-md-7 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold"> نوع المستند : <span class="color_main103">@Model.sDocumentName</span></span>
            </div>
            @*تاريخ نوع المستند*@
            <div class="col-lg-4 col-md-4 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">تاريخ نوع المستند : <span class="color_main103">@Model.sDateDocument</span></span>
            </div>
        </div>
        @* تاريخ بداية العملية *@
        <div class="col-lg-7 col-md-12 col-sm-12 col-12 my-1">
            <span class="color_main102 font-weight-bold">تاريخ بداية العملية : <span class="color_main103">@Model.sDateStartProcess </span></span>
        </div>
        @if (Model.sNameOwnerPermision != "-1")
        {
            @* تاريخ نهاية العملية *@
            <div class="col-lg-5 col-md-12 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">تاريخ نهاية العملية : <span class="color_main103">@Model.sDateEndProcess</span></span>
            </div>
            @* المبلغ المبدئى *@
            <div class="col-lg-7 col-md-12 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold"> القيمه الكليه للعملية : <span class="color_main103">@(String.IsNullOrEmpty(Model.dnFullBudget.ToString()) ? "------------" : Model.dnFullBudget.ToString())</span></span>
            </div>
            @* المبلغ الفعلى *@
            <div class="col-lg-5 col-md-12 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">المبلغ الفعلى : <span class="color_main103">------------</span></span>
            </div>
            @* الحد الأدنى *@
            <div class="col-lg-4 col-md-4 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">الحد الأدنى : <span class="color_main103">------------</span></span>
            </div>
            @* الحد الأقصى *@
            <div class="col-lg-4 col-md-4 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">الحد الأقصى : <span class="color_main103">------------</span></span>
            </div>
            @* العدد الفعلى *@
            <div class="col-lg-4 col-md-4 col-sm-12 col-12 my-1">
                <span class="color_main102 font-weight-bold">العدد الفعلى : <span class="color_main103">------------</span></span>
            </div>
        }
        else if (Model.sNameOwnerPermision == "-1")
        {
            @* مبلغ العمليه *@
            <div class="col-lg-7 col-md-12 col-sm-12 col-12 my-1">
                @if (Model.dnFullBudget == null)
                {
                    <span class="color_main102 font-weight-bold">مبلغ العمليه : <span class="color_main103">لم يحدد بعد</span></span>
                }
                else
                {
                    <span class="color_main102 font-weight-bold">مبلغ العمليه : <span class="color_main103">@Model.dnFullBudget جنيه</span></span>
                }
            </div>
        }
        @* الموقع *@
        <div class="col-lg-12 col-md-6 col-sm-12 col-12  my-1">
            @if (!String.IsNullOrEmpty(Model.sFullSiteAddress))
            {
                <span class="color_main102 font-weight-bold">الموقع : <span class="color_main103"> @Model.sFullSiteAddress</span> </span>
            }
            else
            {
                <span class="color_main102 font-weight-bold">الموقع : <span class="color_main103"> لم يتم التحديد حتى الان</span> </span>
            }
        </div>
        @*@if (Model.sNameOwnerPermision != "-1")
            {

                <div class="col-lg-12 col-md-12 col-sm-12 col-12 my-1">
                    <span class="color_main102 font-weight-bold">دائن : <span class="color_main103">5,000 جنية</span></span>
                </div>
            }*@
    </div>
</div>