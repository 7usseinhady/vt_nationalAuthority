@model List<DataAccessLayer.Models.missionSubContractorModel>
@{
    string[] path = HttpContext.Current.Request.Url.AbsolutePath.Split('/');
    var iCount = 1;
}
<div class="row justify-content-center">
    <div class="scroll-max grid_box_shadow">
        @if (Model == null)
        {
            <h1 id="tblSearchNotFound">
                <strong>@generalVariables.ContractorsMissionNotFound</strong>
            </h1>
        }
        else if (Model.Count == 0)
        {
            <h1 id="tblSearchNotFound">
                <strong>@generalVariables.ContractorsMissionNotFound</strong>
            </h1>
        }
        else if (Model.Count > 0)
        {
            @* table *@
            <table class="table">
                <thead class="text-center">
                    <tr>
                        <th scope="col" class="color_main102 hidCol">#</th>
                        @*<th scope="col" class="color_main102">اسم العمليه</th>*@
                        <th scope="col" class="color_main102">نوعها</th>
                        <th scope="col" class="color_main102">رقم البند</th>
                    </tr>
                </thead>
                <tbody>
                    @{iCount = 1; }
                    @foreach (var item in Model)
                    {
                        <tr style="text-align:center">
                            <th scope="row" class="color_main102">@iCount</th>
                            @*<td class="color_main103">@item.sProcessName</td>*@
                            <td class="color_main103"> @item.sProcessTypeName</td>
                            <td class="color_main103">@item.sBandNumber</td>
                        </tr>
                        iCount++;
                    }
                </tbody>
            </table>
                        }
    </div>
</div>
