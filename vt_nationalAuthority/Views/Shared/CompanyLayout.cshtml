<!DOCTYPE html>
<html>
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="~/img/logo.png">

    <!-- Bootstrap CSS -->
    <script src="~/Scripts/jquery/jquery-3.3.1.js"></script>

    <link href="~/Content/css/all.css" rel="stylesheet" />
    <link href="~/Content/font-family/Kaushan/Kaushan.css" rel="stylesheet" />
    <link href="~/Content/font-family/Modak-Pacifico-Permanent-Marker-Righteous/display-swap/Modak-Pacifico-Permanent-Marker-Righteous.css" rel="stylesheet" />
    <link href="~/Content/font-family/Cairo-Changa-ElMessiri-Tajawal/display-swap/Cairo-Changa-ElMessiri-Tajawal.css" rel="stylesheet" />

    <link href="~/Content/checkbox.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    <link href="~/Content/loaderStyle.css" rel="stylesheet" />
    @Styles.Render("~/bundles/select.css")

</head>
<body>
    <div class="s_b text-center" id="loader" style="display: none;">
        <div class="lds-spinner"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
    </div>
    <div class="loader101 text-center align-content-center" id="loader101">
        <div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div>
    </div>
    <div class="loader102 text-center align-content-center" id="loader102">
        <div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div>
    </div>
    <nav class="navbar navbar-expand-lg navbar-light dr-rtl p-0 box_nav101">
        <div class="logo_border_r bg_g101 w-lg text-right d-flex">
            <div class="row">
                <a class="nav-link list_lg_a  hvr-growt" href="#">
                    <img src="~/img/logo.png" class="img_logo101" style="width:90px;height:90px" />
                    الهيئة القومية للتأمين الإجتماعى
                </a>
            </div>
            <div class="align-self-center d-lg-block d-none d-sm-none d-xl-block list_lg p-2 ">
                <ul class="navbar-nav ">
                    <li class="nav-item active">
                    </li>
                </ul>
            </div>
        </div>

        <button class="navbar-toggler ml-3" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <i class="fas fa-bars"></i>
        </button>

        @* language اللغه *@@* setting *@@* chat *@@* notificatio *@@* user *@
        <div class="d-flex mr-auto icon-nav101 text-center p-3">

            @* chat *@
            @if (Session["contractor"] != null)
            {
                @* notificatio *@
                <div class="col">
                    <a class="text-white101 font102 d-inline-flex" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="position:relative;color:#4cb8d6 !important">
                        <span class="unread_msg_num" style="top:-11px;right:-17px; display:none" id="notificationNumber"></span>
                        <i class="fas fa-bell"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right-chat" aria-labelledby="navbarDropdown" style="background-color:rgb(255, 255, 255);width:270px;">
                        <div id="notification_div1">
                        </div>
                        <div class="dropdown-divider bg-text101 bg-text101"></div>
                        <a class="dropdown-item font102 text-center text_color102" href="#">المزيد ...</a>
                    </div>
                </div>
            }
            @* user *@
            <div class="col">
                <a class="text-white101 font102" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="position:relative;color:#4cb8d6 !important">
                    <i class="fas fa-user"></i>
                </a>
                <div class="dropdown-menu " aria-labelledby="navbarDropdown" style="background-color:#f0f0f0!important;width:min-content;">

                    <a href="#" data-toggle="tooltip" data-placement="bottom" title="go to my profile">
                        <div class="row text-center">
                            <div class="mt-2 mr-5">
                                <p class="text-center">
                                    @if (Session["uImage"] != null)
                                    {
                                        <img src=@Session["uImage"].ToString() id="blah" width="90" height="90" style="border-radius: 50%;" />
                                    }
                                    else
                                    {
                                        <img src="~/UserImage/generic-user.jpg" class="m-auto" width="90" height="90" id="blah" style="border-radius: 50%;" />
                                    }
                                </p>
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                @if (Session["UserName"] != null)
                                {
                                    <strong class="font102  text_color102" style="color:#198ca9!important">@Session["UserName"].ToString()</strong>
                                }
                            </div>
                        </div>
                    </a>
                    <div class="form-group mt-2 mx-2">
                        <a href=@Url.Action("Autherize_logout", "Home") class="btn btn-block" style="background-color: #2291b0; color:white">تسجيل الخروج</a>
                    </div>

                </div>
            </div>
        </div>
    </nav>

    <script>
        function changeCSS(cssFile, cssLinkIndex) {
            var oldlink = document.getElementsByTagName("link").item(cssLinkIndex);
            var newlink = document.createElement("link");
            newlink.setAttribute("rel", "stylesheet");
            newlink.setAttribute("type", "text/css");
            newlink.setAttribute("href", cssFile);
            document.getElementsByTagName("head").item(0).replaceChild(newlink, oldlink);
        }
    </script>

    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>

    <script src="~/Scripts/bootstrap-4.1.3/bootstrap.min.js"></script>

    @* Drop Down List *@
    @Scripts.Render("~/bundles/select.js")
    <script src="~/Scripts/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/light.js"></script>
    <div class="Mb-5">
        @RenderBody()
    </div>

    @* Shared Modal *@
    <div class="modal" id="sharedModal">
        <div class="modal-dialog modal-lg dr-rtl">
            <div class="modal-content">
                <div class="modal-header header">
                    <h5 class="modal-title color_main102" id="titleSharedModal"></h5>
                    <button class="close color_main102" data-dismiss="modal" style="position: absolute;left: 0;">&times;</button>
                </div>
                <div class="modal-body" id="sharedModalBody">
                </div>
            </div>
        </div>
    </div>

    @*Share Seprated Mode*@
    <div class="modal" id="sharedSepratedModal">
        <div class="modal-dialog modal-lg dr-rtl">
            <div class="modal-content" style="background-color: transparent!important">
                <div class="modal-header header">
                    <h5 class="modal-title color_main102" id="titleSharedSepratedModal"></h5>
                    <button class="close color_main102" data-dismiss="modal" style="position: absolute;left: 0;">&times;</button>
                </div>
                <div class="col-12 mx-0 px-0" id="sharedSepratedModalBody">
                </div>
            </div>
        </div>
    </div>

    @* Shared Attachment Search - Add *@
    <div class="modal dr-rtl" id="shareAttachmentSearchAddModal">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="background-color: transparent!important;">
                <div class="modal-header header">
                    <h5 class="modal-title color_main102" id="titleSharedAttachmentSearchAddModal">بحث - اضافه مرفق</h5>
                    <button class="close color_main102" data-dismiss="modal" style="position: absolute;left: 0;">&times;</button>
                </div>
                <div class="modal-body bg-white p-3">
                    <div class="container">
                        <div class="row justify-content-center">
                            <button type="button" id="btnSharedAttachmentSearch" class="btn btn-outline-primary font-weight-bold" onclick="displayDivs('#divDisplaySearchAttachment',this)"><i class="fas fa-search"></i>  بحث عن مرفق</button>
                            <button type="button" id="btnSharedAttachmentAdd" class="btn btn-outline-info font-weight-bold" onclick="displayDivs('#divDisplayAddAttachment',this)"><i class="fas fa-plus"></i> اضافة مرفق</button>
                        </div>
                    </div>
                </div>
                <div id="divDisplaySharedSearchAddAttchment" class="col-12 px-0 mx-0">
                </div>
            </div>
        </div>
    </div>

    @* Shared Attachment Search *@
    <div class="modal dr-rtl" id="shareAttachmentSearchModal">
        <div class="modal-dialog modal-lg">
            <div class="modal-content" style="background-color: transparent!important;">
                <div class="modal-header header">
                    <h5 class="modal-title color_main102" id="titleSharedAttachmentSearchModal"></h5>
                    <button class="close color_main102" data-dismiss="modal" style="position: absolute;left: 0;">&times;</button>
                </div>
                <div class="modal-body bg-white p-3">
                </div>
                <div id="divDisplaySharedSearchAttchment" class="col-12 px-0 mx-0">
                </div>
            </div>
        </div>
    </div>

    <style>
        .guidColor {
            position: fixed;
            bottom: 0;
            left: -21px;
            padding-left: 2%;
            padding-bottom: 10px;
        }
    </style>

    <script>
        $(window).on('load', function () {
            $('#loader102').fadeOut();
        });
    </script>

</body>
</html>